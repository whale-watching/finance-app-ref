{
    "compilerOptions": {
        "outDir": "./build/",
        "sourceMap": true,
        "strictNullChecks": true,
        "module": "es6",
        "jsx": "react",
        "target": "es5",
        "allowJs": true,
        "allowSyntheticDefaultImports": true,
        "moduleResolution": "node",
        "noImplicitAny": true
    },
    "include": [
        "./src/"
    ]
}